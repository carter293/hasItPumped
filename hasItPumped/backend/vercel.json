{
    "version": 2,
    "builds": [
      {
        "src": "src/solana_token_api/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/solana_token_api/main.py"
      }
    ],
    "env": {
      "APP_MODULE": "src.solana_token_api.main:app"
    }
  }